{
  "name": "opentelemetry",
  "version": "0.0.15",
  "version.name": "collector-browser-traceparent",
  "source": "./src/private/index.html",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "author": "Tom S. - HH | thoschulte@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run up",
    "re:start": "npm run down && npm start",
    "docker:ps": "docker ps -a",
    "up": "docker compose up --force-recreate",
    "up:d": "docker compose up --force-recreate -d",
    "down": "docker compose down",
    "test": "echo \"Error: no test specified\" && exit 1",
    "todo": "ts-node-dev --inspect --watch -- src/todo-service.ts",
    "auth": "ts-node-dev --inspect -- src/auth-service.ts",
    "ui": "ts-node-dev --inspect -- src/ui-service.ts",
    "build:jspm": "npx jspm link ./src/public/index.html -o src/public/views/index.html",
    "build:parcel": "npm run clean:.parcel-cache && parcel build --dist-dir src/public/views/partials",
    "build": "npm run clean:/src/public && npm run build:parcel && npm run cpy",
    "cpy": "npx cpy 'src/public/views/partials/*.html' src/public/views/layouts --rename=main.handlebars",
    "watch": "npx watch 'yarn start' .",
    "clean": "npx concurrently 'npm run clean:node_modules'",
    "clean:node_modules": "rimraf node_modules",
    "clean:.parcel-cache": "rimraf .parcel-cache",
    "clean:/src/public/partials/views/index.html": "rimraf src/public/views/partials/index.html",
    "clean:/src/public/views/layouts/main.handlebars": "rimraf src/public/views/layouts/main.handlebars",
    "clean:/src/public": "npm run clean:/src/public/views/layouts/main.handlebars && npm run clean:/src/public/partials/views/index.html"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/api-logs": "^0.45.1",
    "@opentelemetry/auto-instrumentations-node": "^0.39.2",
    "@opentelemetry/auto-instrumentations-web": "^0.34.0",
    "@opentelemetry/context-zone": "^1.17.1",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-logs-otlp-http": "0.44.0",
    "@opentelemetry/exporter-logs-otlp-proto": "^0.45.1",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.45.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.44.0",
    "@opentelemetry/exporter-prometheus": "^0.44.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.46.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.43.0",
    "@opentelemetry/instrumentation": "^0.46.0",
    "@opentelemetry/instrumentation-document-load": "^0.33.2",
    "@opentelemetry/instrumentation-fetch": "^0.46.0",
    "@opentelemetry/instrumentation-long-task": "^0.34.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.33.2",
    "@opentelemetry/instrumentation-xml-http-request": "^0.44.0",
    "@opentelemetry/resource-detector-docker": "^0.1.2",
    "@opentelemetry/resources": "^1.19.0",
    "@opentelemetry/sdk-logs": "^0.45.1",
    "@opentelemetry/sdk-metrics": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.43.0",
    "@opentelemetry/sdk-trace-base": "^1.19.0",
    "@opentelemetry/sdk-trace-web": "^1.17.1",
    "@opentelemetry/semantic-conventions": "^1.19.0",
    "@types/morgan": "^1.9.9",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "cpy-cli": "^5.0.0",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-winston": "^4.2.0",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0",
    "parcel": "^2.10.3",
    "ramda": "^0.29.1",
    "rimraf": "^5.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.17",
    "@types/ramda": "^0.29.7",
    "process": "^0.11.10",
    "tsc-watch": "^6.0.4"
  },
  "description": "This repository contains a demo application in terms of Observability in Cloud Native and Frontend Apps using OpenTelemetry",
  "repository": "https://github.com/thoschu/opentelemetry",
  "private": true
}
