{
  "name": "opentelemetry",
  "version": "0.0.12",
  "source": "./src/private/index.html",
  "browserslist": ["> 0.5%", "last 2 versions", "not dead"],
  "author": "Tom S. - HH | thoschulte@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run up",
    "docker:ps": "docker ps -a",
    "up": "docker compose up --force-recreate",
    "down": "docker compose down",
    "test": "echo \"Error: no test specified\" && exit 1",
    "todo": "ts-node-dev --inspect --watch -- src/todo-service.ts",
    "auth": "ts-node-dev --inspect -- src/auth-service.ts",
    "ui": "ts-node-dev --inspect -- src/ui-service.ts",
    "watch": "npx watch 'yarn start' .",
    "build": "npm run clean:.parcel-cache && npm run clean:src/public && parcel build --dist-dir src/public",
    "clean": "npx concurrently 'npm run clean:node_modules' 'npm run clean:yarn.lock'",
    "clean:node_modules": "npx rimraf node_modules",
    "clean:src/public": "npx concurrently 'npx rimraf src/public/*.js' 'npx rimraf src/public/*.map' 'npx rimraf src/public/*.html'",
    "clean:.parcel-cache": "npx rimraf .parcel-cache"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/auto-instrumentations-node": "^0.39.2",
    "@opentelemetry/context-zone": "^1.17.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.44.0",
    "@opentelemetry/exporter-prometheus": "^0.43.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.43.0",
    "@opentelemetry/instrumentation-document-load": "^0.33.2",
    "@opentelemetry/instrumentation-user-interaction": "^0.33.2",
    "@opentelemetry/instrumentation-xml-http-request": "^0.44.0",
    "@opentelemetry/resource-detector-docker": "^0.1.2",
    "@opentelemetry/sdk-metrics": "^1.17.0",
    "@opentelemetry/sdk-node": "^0.43.0",
    "@opentelemetry/sdk-trace-web": "^1.17.1",
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "ramda": "^0.29.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.17",
    "@types/ramda": "^0.29.7",
    "parcel": "^2.10.1",
    "process": "^0.11.10",
    "tsc-watch": "^6.0.4"
  },
  "description": "This repository contains a demo application in terms of Observability in Cloud Native and Frontend Apps using OpenTelemetry",
  "repository": "https://github.com/thoschu/opentelemetry",
  "private": true
}
